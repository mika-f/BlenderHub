{
  "name": "blender-hub",
  "productName": "blender-hub",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "test": "jest",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --write ./src",
    "lint:eslint": "eslint --format friendly ./src",
    "schema": "run-p schema:*",
    "schema:config": "typescript-json-schema --noExtraProps --required ./tsconfig.json IConfiguration --out ./src/main/schema/configuration.json",
    "schema:install": "typescript-json-schema --noExtraProps --required ./tsconfig.json IInstallations --out ./src/main/schema/installations.json"
  },
  "keywords": [],
  "author": {
    "name": "Natsuneko",
    "email": "me@mochizuki.moe"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "blender_hub"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload/index.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/adm-zip": "^0.4.34",
    "@types/cheerio": "^0.22.28",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "autoprefixer": "^10.2.5",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^4.2.1",
    "electron": "12.0.5",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "jest": "^26.6.3",
    "node-loader": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.12",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "react-refresh": "^0.10.0",
    "react-refresh-typescript": "^2.0.0",
    "style-loader": "^1.2.1",
    "tailwindcss": "^2.1.1",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2",
    "typescript-json-schema": "^0.50.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "adm-zip": "^0.5.5",
    "ajv": "^8.1.0",
    "cheerio": "^1.0.0-rc.6",
    "electron-dl": "^3.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "extract-dmg": "^1.0.0",
    "got": "^11.8.2",
    "jsonfile": "^6.1.0",
    "querystring": "^0.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hooks-global-state": "^1.0.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-spinners": "^0.10.6",
    "update-electron-app": "^2.0.1"
  }
}
